{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "appointment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "reason": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Confirmed",
        "Rescheduled",
        "Cancelled"
      ]
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "appointments"
    },
    "doctors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::doctor.doctor",
      "inversedBy": "appointments"
    }
  }
}
